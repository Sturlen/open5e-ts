{
    "name": "@sturlen/open5e",
    "version": "1.0.0-1",
    "license": "MIT",
    "description": "Typesafe Ope5e API wrapper using Zod",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        },
        "./monsters": {
            "types": "./dist/monsters.d.ts",
            "require": "./dist/monsters.js",
            "import": "./dist/monsters.mjs"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/*"
            ]
        }
    },
    "scripts": {
        "build": "tsup",
        "test": "vitest",
        "format": "prettier --write .",
        "release": "release-it"
    },
    "keywords": [
        "5e",
        "dnd",
        "open5e",
        "zod"
    ],
    "author": "Sturlen",
    "files": [
        "/dist"
    ],
    "repository": "https://github.com/Sturlen/open5e-typesafe-api/",
    "devDependencies": {
        "@swc/core": "^1.3.104",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.4",
        "fetch-mock": "^9.11.0",
        "node-fetch": "2.6.1",
        "release-it": "^17.0.3",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3",
        "prettier": "^3.2.5",
        "vitest": "^1.2.2"
    },
    "dependencies": {
        "zod": "^3.22.4"
    }
}
