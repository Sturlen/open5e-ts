{
    "name": "open5e",
    "version": "0.0.0",
    "description": "Typesafe Ope5e API wrapper using Zod",
    "main": "./dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --clean --target es5",
        "build:watch": "tsup src/index.ts --format cjs,esm --clean --watch --target es5",
        "test": "jest",
        "release": "release-it"
    },
    "keywords": [
        "5e",
        "dnd",
        "open5e",
        "zod"
    ],
    "author": "Sturlen",
    "files": [
        "/dist"
    ],
    "license": "MIT",
    "repository": "https://github.com/Sturlen/convert/",
    "devDependencies": {
        "@swc/core": "^1.3.104",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.4",
        "jest": "^29.7.0",
        "release-it": "^17.0.1",
        "ts-jest": "^29.1.1",
        "tsup": "^8.0.1",
        "typescript": "^5.3.3"
    }
}
